{
  "name": "@scrowl/template-block-text",
  "version": "1.0.0",
  "meta": {
    "label": "Text Block",
    "component": "BlockText",
    "filename": "block-text"
  },
  "description": "",
  "license": "MIT",
  "keywords": [],
  "author": {
    "name": "OSG"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scrowl-io/authoring.git",
    "directory": "packages/template/block-text"
  },
  "bugs": {
    "url": "https://github.com/scrowl-io/scrowl/issues"
  },
  "homepage": "https://github.com/scrowl-io/authoring/packages/template/block-text#readme",
  "scripts": {
    "clean": "rimraf ../.parcel-cache",
    "start": "parcel serve --target dev --dist-dir ./dev --no-cache --open",
    "prebuild": "yarn run clean && rimraf dist",
    "build": "parcel build --target prod --no-cache"
  },
  "main": "./src/index.ts",
  "prod": "./dist/scrowl.template-block-text.js",
  "alias": {
    "react": {
      "global": "React"
    },
    "@scrowl/template-core": {
      "global": "TemplateCore"
    }
  },
  "targets": {
    "prod": {
      "source": "./src/prod.ts",
      "outputFormat": "global",
      "optimize": false
    },
    "dev": {
      "source": "./src/dev/index.html",
      "distDir": "./dev"
    }
  },
  "exports": {
    ".": {
      "require": "./dist/scrowl.template-block-text.js",
      "default": "./dist/scrowl.template-block-text.js",
      "import": "./src/index.ts"
    },
    "./src": "./src/index.ts",
    "./template": "./dist/scrowl.template-block-text.js",
    "./meta.json": "./src/meta.json"
  },
  "eslintConfig": {
    "extends": "@scrowl/config"
  },
  "prettier": "@scrowl/config/prettier",
  "stylelint": {
    "extends": "@scrowl/config/stylelint"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/",
    "package.json"
  ],
  "devDependencies": {
    "@parcel/core": "2.7.0",
    "@parcel/packager-ts": "2.7.0",
    "@parcel/reporter-dev-server": "2.7.0",
    "@parcel/runtime-react-refresh": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@parcel/validator-typescript": "2.7.0",
    "@scrowl/config": "^0.0.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.25.0",
    "parcel": "2.7.0",
    "postcss": "^8.4.17",
    "postcss-import": "^14.1.0",
    "postcss-modules": "^4.3.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stylelint": "^14.13.0",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "@owlui/lib": "^0.0.44",
    "@scrowl/template-core": "^1.0.0",
    "react": "^18.2.0"
  }
}
